<div id="<%= dom_id usuario %>" class="card mb-4 shadow-sm">
  <div class="card-body">
    <h5 class="card-title mb-3"><%= usuario.nome %></h5>

    <p class="card-text mb-1">
      <strong>Email:</strong> <%= usuario.email %>
    </p>

    <p class="card-text">
      <strong>Telefone:</strong> <%= usuario.telefone %>
    </p>
  </div>
</div>
